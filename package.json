{
  "name": "squadlink",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "type-check": "tsc --noEmit",
    "build:android": "./scripts/build-android.sh release",
    "build:android:debug": "./scripts/build-android.sh debug",
    "build:clean": "cd android && ./gradlew clean",
    "release:prepare": "./scripts/create-release.sh",
    "release:finalize": "./scripts/finalize-release.sh",
    "release:help": "echo 'Étape 1: npm run release:prepare <version> | Étape 2: Créer MR avec version code/name | Étape 3: npm run release:finalize <version>'"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "<rootDir>/components/__tests__/setup.ts",
      "<rootDir>/components/__tests__/utils/testUtils.tsx"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "2.50.5",
    "@testing-library/react-native": "^13.2.0",
    "core-js": "^3.44.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "expo": "~53.0.17",
    "expo-constants": "^17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-location": "^18.1.6",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.31.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint-config-expo": "^9.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "@types/testing-library__react": "^10.0.1",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
