{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "test": {
      "distribution": "internal",
      "env": {
        "NODE_ENV": "test"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug :app:assembleAndroidTest"
      }
    }
  },
  "submit": {
    "production": {}
  },
  "test": {
    "maestro": {
      "flows": [
        ".maestro/"
      ]
    }
  }
}
